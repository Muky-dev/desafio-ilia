generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Alocacao {
  id     Int    @id @default(autoincrement())
  nomeProjeto String
  dia    String
  tempo  String
}

model BatidaPonto {
  id     Int    @id @default(autoincrement())
  dia    String
  horario  String
}
